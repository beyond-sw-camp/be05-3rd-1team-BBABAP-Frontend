.author = '익명';\r\n\r\n      axios.post('http://localhost:7777/boards', this.post, {\r\n        headers: {\r\n          'Content-Type': 'application/json'\r\n        }\r\n      })\r\n          .then(() => {\r\n            this.$router.push('/boardlist');\r\n          })\r\n          .catch(error => {\r\n            console.error('Error creating post:', error);\r\n          });\r\n    },\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.post-form-container {\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n}\r\n\r\n.post-form {\r\n  background-color: #f9f9f9;\r\n  padding: 20px;\r\n  border-radius: 5px;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 15px;\r\n}\r\n\r\nlabel {\r\n  display: block;\r\n  font-weight: bold;\r\n}\r\n\r\ninput[type=\"text\"] {\r\n  width: 100%;\r\n  padding: 10px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 5px;\r\n  box-sizing: border-box;\r\n}\r\n\r\nbutton.submit-button {\r\n  background-color: #007bff;\r\n  color: #fff;\r\n  padding: 10px 20px;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\nbutton.submit-button:hover {\r\n  background-color: #0056b3;\r\n}\r\n</style>\r\n"],"mappings":";AAqBA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,CAAA,MAAO,QAAQ;AACtB;AACA;AACA,OAAO,oCAAoC,EAAE;AAC7C,OAAO,gCAAgC,EAAE;;AAEzC,eAAe;EACbC,UAAU,EAAE;IACV;EAAA,CACD;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,KAAK,EAAE,EAAE;QACTC,OAAO,EAAE,EAAE;QACXC,UAAU,EAAE,EAAE;QAAE;QAChBC,YAAY,EAAC,EAAE;QACfC,MAAM,EAAE;MACV,CAAC;MACDC,YAAY,EAAE;QACZ;MAAA;IAEJ,CAAC;EACH,CAAC;EAEDC,OAAOA,CAAA,EAAG;IACRC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACH,YAAY,CAAC;EAChC,CAAC;EAEDI,OAAOA,CAAA,EAAG;IACR;IACAb,CAAC,CAAC,aAAa,CAAC,CAACc,UAAU,CAAC;MAC1BC,MAAM,EAAE,GAAG;MAAE;MACbC,SAAS,EAAE;QACTC,QAAQ,EAAGC,QAAQ,IAAK;UACtB;UACA,IAAI,CAACf,IAAI,CAACE,OAAM,GAAIa,QAAQ;QAC9B;MACF;IACF,CAAC,CAAC;EACJ,CAAC;EAEDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX;MACA,IAAI,CAACjB,IAAI,CAACG,UAAS,GAAI,IAAIe,IAAI,CAAC,CAAC,CAACC,WAAW,CAAC,CAAC;MAC/C,IAAI,CAACnB,IAA