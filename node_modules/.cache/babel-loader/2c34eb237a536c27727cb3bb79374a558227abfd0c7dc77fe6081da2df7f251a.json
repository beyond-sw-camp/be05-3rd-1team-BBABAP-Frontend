{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport { mapGetters } from 'vuex';\nimport $ from 'jquery';\nimport 'bootstrap';\nimport 'summernote/dist/summernote-lite.css';\nimport 'summernote/dist/summernote-lite';\nexport default {\n  created() {\n    this.fetchPost();\n  },\n  computed: {\n    // userNickname getter를 가져옴\n    ...mapGetters(['userNickname'])\n  },\n  data() {\n    return {\n      post: {\n        title: '',\n        content: '',\n        created_at: '',\n        // 추가: 현재 시간을 저장할 속성,\n        created_date: '',\n        author: ''\n      }\n    };\n  },\n  mounted() {\n    // Summernote 초기화\n    $('#summernote').summernote({\n      height: 300,\n      // 높이 설정 (원하는 대로 조절 가능)\n      callbacks: {\n        onChange: contents => {\n          // 내용이 변경될 때마다 데이터에 저장\n          this.post.content = contents;\n        }\n      }\n    });\n  },\n  methods: {\n    fetchPost() {\n      const boardId = this.$route.params.id;\n      axios.get(`http://localhost:7777/boards/${boardId}`).then(response => {\n        this.updatedPost.title = response.data.title;\n        this.updatedPost.content = response.data.content;\n      }).catch(error => {\n        console.error('Error fetching post:', error);\n      });\n    },\n    submitForm() {\n      // 현재 시간을 설정\n      this.post.created_at = new Date().toISOString();\n      this.post.created_date = this.post.created_at.slice(0, 10);\n      this.post.author = this.userNickname;\n      console.log(this.userNickname);\n      axios.post('http://localhost:7777/boards', this.post, {\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      }).then(() => {\n        this.$router.push('/boardlist');\n      }).catch(error => {\n        console.error('Error creating post:', error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","mapGetters","$","created","fetchPost","computed","data","post","title","content","created_at","created_date","author","mounted","summernote","height","callbacks","onChange","contents","methods","boardId","$route","params","id","get","then","response","updatedPost","catch","error","console","submitForm","Date","toISOString","slice","userNickname","log","headers","$router","push"],"sources":["C:\\encore\\be05-3rd-1team-BBABAP-Frontend\\src\\views\\PostForm.vue"],"sourcesContent":["<template>\r\n    <div class=\"post-form-container\">\r\n        <h2>게시물 작성</h2>\r\n        <form @submit.prevent=\"submitForm\" class=\"post-form\">\r\n            <div class=\"form-group\">\r\n                <label for=\"title\">제목:</label>\r\n                <input type=\"text\" id=\"title\" v-model=\"post.title\" required>\r\n            </div>\r\n            <div class=\"form-group\">\r\n                <label for=\"content\">내용:</label>\r\n                <textarea id=\"summernote\" v-model=\"post.content\" placeholder=\"내용을 입력하세요\"></textarea>\r\n            </div>\r\n            <button type=\"submit\" class=\"submit-button\">작성</button>\r\n        </form>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { mapGetters } from 'vuex';\r\n\r\nimport $ from 'jquery';\r\nimport 'bootstrap';\r\n\r\nimport 'summernote/dist/summernote-lite.css';\r\nimport 'summernote/dist/summernote-lite';\r\n\r\nexport default {\r\n    created() {\r\n      this.fetchPost();\r\n    },\r\n\r\n    computed: {\r\n    // userNickname getter를 가져옴\r\n    ...mapGetters(['userNickname'])\r\n  },\r\n\r\n    data() {\r\n        return {\r\n            post: {\r\n                title: '',\r\n                content: '',\r\n                created_at: '', // 추가: 현재 시간을 저장할 속성,\r\n                created_date:'',\r\n                author: '',\r\n            },\r\n        };\r\n    },\r\n    mounted() {\r\n      // Summernote 초기화\r\n      $('#summernote').summernote({\r\n        height: 300, // 높이 설정 (원하는 대로 조절 가능)\r\n        callbacks: {\r\n          onChange: (contents) => {\r\n            // 내용이 변경될 때마다 데이터에 저장\r\n            this.post.content = contents;\r\n          }\r\n        }\r\n      });\r\n    },\r\n    methods: {\r\n          fetchPost() {\r\n          const boardId = this.$route.params.id;\r\n          axios.get(`http://localhost:7777/boards/${boardId}`)\r\n            .then(response => {\r\n              this.updatedPost.title = response.data.title;\r\n              this.updatedPost.content = response.data.content;\r\n            })\r\n            .catch(error => {\r\n              console.error('Error fetching post:', error);\r\n            });\r\n        },\r\n        submitForm() {\r\n            // 현재 시간을 설정\r\n            this.post.created_at = new Date().toISOString();\r\n            this.post.created_date = this.post.created_at.slice(0, 10);\r\n            this.post.author = this.userNickname;\r\n            console.log(this.userNickname);\r\n            \r\n            axios.post('http://localhost:7777/boards', this.post, {\r\n                headers: {\r\n                    'Content-Type': 'application/json'\r\n                }\r\n            })\r\n            .then(() => {\r\n                this.$router.push('/boardlist');  \r\n            })\r\n            .catch(error => {\r\n                console.error('Error creating post:', error);\r\n            });\r\n        },\r\n    }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.post-form-container {\r\n    max-width: 600px;\r\n    margin: 0 auto;\r\n    padding: 20px;\r\n}\r\n\r\n.post-form {\r\n    background-color: #f9f9f9;\r\n    padding: 20px;\r\n    border-radius: 5px;\r\n}\r\n\r\n.form-group {\r\n    margin-bottom: 15px;\r\n}\r\n\r\nlabel {\r\n    display: block;\r\n    font-weight: bold;\r\n}\r\n\r\ninput[type=\"text\"],\r\ntextarea {\r\n    width: 100%;\r\n    padding: 10px;\r\n    border: 1px solid #ccc;\r\n    border-radius: 5px;\r\n    box-sizing: border-box;\r\n}\r\n\r\nbutton.submit-button {\r\n    background-color: #007bff;\r\n    color: #fff;\r\n    padding: 10px 20px;\r\n    border: none;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n}\r\n\r\nbutton.submit-button:hover {\r\n    background-color: #0056b3;\r\n}\r\n</style>\r\n"],"mappings":";AAkBA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,UAAS,QAAS,MAAM;AAEjC,OAAOC,CAAA,MAAO,QAAQ;AACtB,OAAO,WAAW;AAElB,OAAO,qCAAqC;AAC5C,OAAO,iCAAiC;AAExC,eAAe;EACXC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,SAAS,CAAC,CAAC;EAClB,CAAC;EAEDC,QAAQ,EAAE;IACV;IACA,GAAGJ,UAAU,CAAC,CAAC,cAAc,CAAC;EAChC,CAAC;EAECK,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,IAAI,EAAE;QACFC,KAAK,EAAE,EAAE;QACTC,OAAO,EAAE,EAAE;QACXC,UAAU,EAAE,EAAE;QAAE;QAChBC,YAAY,EAAC,EAAE;QACfC,MAAM,EAAE;MACZ;IACJ,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR;IACAX,CAAC,CAAC,aAAa,CAAC,CAACY,UAAU,CAAC;MAC1BC,MAAM,EAAE,GAAG;MAAE;MACbC,SAAS,EAAE;QACTC,QAAQ,EAAGC,QAAQ,IAAK;UACtB;UACA,IAAI,CAACX,IAAI,CAACE,OAAM,GAAIS,QAAQ;QAC9B;MACF;IACF,CAAC,CAAC;EACJ,CAAC;EACDC,OAAO,EAAE;IACHf,SAASA,CAAA,EAAG;MACZ,MAAMgB,OAAM,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAE;MACrCvB,KAAK,CAACwB,GAAG,CAAE,gCAA+BJ,OAAQ,EAAC,EAChDK,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACC,WAAW,CAACnB,KAAI,GAAIkB,QAAQ,CAACpB,IAAI,CAACE,KAAK;QAC5C,IAAI,CAACmB,WAAW,CAAClB,OAAM,GAAIiB,QAAQ,CAACpB,IAAI,CAACG,OAAO;MAClD,CAAC,EACAmB,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,sBAAsB,EAAEA,KAAK,CAAC;MAC9C,CAAC,CAAC;IACN,CAAC;IACDE,UAAUA,CAAA,EAAG;MACT;MACA,IAAI,CAACxB,IAAI,CAACG,UAAS,GAAI,IAAIsB,IAAI,CAAC,CAAC,CAACC,WAAW,CAAC,CAAC;MAC/C,IAAI,CAAC1B,IAAI,CAACI,YAAW,GAAI,IAAI,CAACJ,IAAI,CAACG,UAAU,CAACwB,KAAK,CAAC,CAAC,EAAE,EAAE,CAAC;MAC1D,IAAI,CAAC3B,IAAI,CAACK,MAAK,GAAI,IAAI,CAACuB,YAAY;MACpCL,OAAO,CAACM,GAAG,CAAC,IAAI,CAACD,YAAY,CAAC;MAE9BnC,KAAK,CAACO,IAAI,CAAC,8BAA8B,EAAE,IAAI,CAACA,IAAI,EAAE;QAClD8B,OAAO,EAAE;UACL,cAAc,EAAE;QACpB;MACJ,CAAC,EACAZ,IAAI,CAAC,MAAM;QACR,IAAI,CAACa,OAAO,CAACC,IAAI,CAAC,YAAY,CAAC;MACnC,CAAC,EACAX,KAAK,CAACC,KAAI,IAAK;QACZC,OAAO,CAACD,KAAK,CAAC,sBAAsB,EAAEA,KAAK,CAAC;MAChD,CAAC,CAAC;IACN;EACJ;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}