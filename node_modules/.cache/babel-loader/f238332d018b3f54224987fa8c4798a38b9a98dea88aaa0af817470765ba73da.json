{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport 'summernote/dist/summernote-bs4.css'; // Summernote 스타일시트 import\nimport 'summernote'; // Summernote 라이브러리 import\n\nexport default {\n  data() {\n    return {\n      post: {\n        title: '',\n        content: '',\n        created_at: '',\n        // 추가: 현재 시간을 저장할 속성,\n        created_date: '',\n        author: ''\n      }\n    };\n  },\n  mounted() {\n    // Summernote 에디터 초기화\n    this.initializeSummernote();\n  },\n  methods: {\n    initializeSummernote() {\n      const $summernote = $(this.$refs.summernote);\n      $summernote.summernote({\n        height: 200,\n        // 에디터 높이 설정\n        placeholder: '내용을 입력해주세요...' // Placeholder 설정\n      });\n      // Summernote 에디터 값이 변경될 때마다 post.content 업데이트\n      $summernote.on('summernote.change', () => {\n        this.post.content = $summernote.summernote('code');\n      });\n    },\n    submitForm() {\n      // 현재 시간을 설정\n      this.post.created_at = new Date().toISOString();\n      this.post.created_date = this.post.created_at.slice(0, 10);\n      this.post.author = '익명';\n      axios.post('http://localhost:7777/boards', this.post, {\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      }).then(() => {\n        this.$router.push('/boardlist');\n      }).catch(error => {\n        console.error('Error creating post:', error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","data","post","title","content","created_at","created_date","author","mounted","initializeSummernote","methods","$summernote","$","$refs","summernote","height","placeholder","on","submitForm","Date","toISOString","slice","headers","then","$router","push","catch","error","console"],"sources":["C:\\encore\\be05-3rd-1team-BBABAP-Frontend\\src\\views\\PostForm.vue"],"sourcesContent":["<template>\r\n    <div class=\"post-form-container\">\r\n        <h2>게시물 작성</h2>\r\n        <form @submit.prevent=\"submitForm\" class=\"post-form\">\r\n            <div class=\"form-group\">\r\n                <label for=\"title\">제목:</label>\r\n                <input type=\"text\" id=\"title\" v-model=\"post.title\" required>\r\n            </div>\r\n            <div class=\"form-group\">\r\n                <label for=\"content\">내용:</label>\r\n              <!-- Summernote 에디터로 대체 -->\r\n              <textarea id=\"summernote\" ref=\"summernote\" v-model=\"post.content\" required></textarea>\r\n            </div>\r\n            <button type=\"submit\" class=\"submit-button\">작성</button>\r\n        </form>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport 'summernote/dist/summernote-bs4.css'; // Summernote 스타일시트 import\r\nimport 'summernote'; // Summernote 라이브러리 import\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            post: {\r\n                title: '',\r\n                content: '',\r\n                created_at: '', // 추가: 현재 시간을 저장할 속성,\r\n                created_date:'',\r\n                author: '',\r\n            },\r\n        };\r\n    },\r\n    mounted() {\r\n      // Summernote 에디터 초기화\r\n      this.initializeSummernote();\r\n    },\r\n    methods: {\r\n        initializeSummernote() {\r\n          const $summernote = $(this.$refs.summernote);\r\n          $summernote.summernote({\r\n            height: 200, // 에디터 높이 설정\r\n            placeholder: '내용을 입력해주세요...', // Placeholder 설정\r\n          });\r\n          // Summernote 에디터 값이 변경될 때마다 post.content 업데이트\r\n          $summernote.on('summernote.change', () => {\r\n            this.post.content = $summernote.summernote('code');\r\n          });\r\n        },\r\n        submitForm() {\r\n            // 현재 시간을 설정\r\n            this.post.created_at = new Date().toISOString();\r\n            this.post.created_date = this.post.created_at.slice(0, 10);\r\n            this.post.author = '익명';\r\n            \r\n            axios.post('http://localhost:7777/boards', this.post, {\r\n                headers: {\r\n                    'Content-Type': 'application/json'\r\n                }\r\n            })\r\n            .then(() => {\r\n                this.$router.push('/boardlist');  \r\n            })\r\n            .catch(error => {\r\n                console.error('Error creating post:', error);\r\n            });\r\n        },\r\n    }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.post-form-container {\r\n    max-width: 600px;\r\n    margin: 0 auto;\r\n    padding: 20px;\r\n}\r\n\r\n.post-form {\r\n    background-color: #f9f9f9;\r\n    padding: 20px;\r\n    border-radius: 5px;\r\n}\r\n\r\n.form-group {\r\n    margin-bottom: 15px;\r\n}\r\n\r\nlabel {\r\n    display: block;\r\n    font-weight: bold;\r\n}\r\n\r\ninput[type=\"text\"],\r\ntextarea {\r\n    width: 100%;\r\n    padding: 10px;\r\n    border: 1px solid #ccc;\r\n    border-radius: 5px;\r\n    box-sizing: border-box;\r\n}\r\n\r\nbutton.submit-button {\r\n    background-color: #007bff;\r\n    color: #fff;\r\n    padding: 10px 20px;\r\n    border: none;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n}\r\n\r\nbutton.submit-button:hover {\r\n    background-color: #0056b3;\r\n}\r\n</style>\r\n"],"mappings":";AAmBA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAO,oCAAoC,EAAE;AAC7C,OAAO,YAAY,EAAE;;AAErB,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,IAAI,EAAE;QACFC,KAAK,EAAE,EAAE;QACTC,OAAO,EAAE,EAAE;QACXC,UAAU,EAAE,EAAE;QAAE;QAChBC,YAAY,EAAC,EAAE;QACfC,MAAM,EAAE;MACZ;IACJ,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR;IACA,IAAI,CAACC,oBAAoB,CAAC,CAAC;EAC7B,CAAC;EACDC,OAAO,EAAE;IACLD,oBAAoBA,CAAA,EAAG;MACrB,MAAME,WAAU,GAAIC,CAAC,CAAC,IAAI,CAACC,KAAK,CAACC,UAAU,CAAC;MAC5CH,WAAW,CAACG,UAAU,CAAC;QACrBC,MAAM,EAAE,GAAG;QAAE;QACbC,WAAW,EAAE,eAAe,CAAE;MAChC,CAAC,CAAC;MACF;MACAL,WAAW,CAACM,EAAE,CAAC,mBAAmB,EAAE,MAAM;QACxC,IAAI,CAACf,IAAI,CAACE,OAAM,GAAIO,WAAW,CAACG,UAAU,CAAC,MAAM,CAAC;MACpD,CAAC,CAAC;IACJ,CAAC;IACDI,UAAUA,CAAA,EAAG;MACT;MACA,IAAI,CAAChB,IAAI,CAACG,UAAS,GAAI,IAAIc,IAAI,CAAC,CAAC,CAA